    <script>
        $(document).ready(function () {
            let counted = false;

            $(window).on('scroll', function () {
                let oTop = $('#achievements').offset().top - window.innerHeight;

                if (!counted && $(window).scrollTop() > oTop) {
                    $('.counter-number').each(function () {
                        let $this = $(this);
                        let target = parseInt($this.attr('data-target'));
                        let count = 0;
                        let speed = 2000; // total duration in ms
                        let step = Math.ceil(target / (speed / 20));

                        let counter = setInterval(function () {
                            count += step;
                            if (count >= target) {
                                count = target;
                                clearInterval(counter);
                            }
                            $this.text(count + '+');
                        }, 20);
                    });

                    counted = true;
                }
            });
        });
    </script>

