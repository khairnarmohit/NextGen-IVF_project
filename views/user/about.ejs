<%- include("navbar.ejs") %>
    <style>
        /* Internal CSS for About Page */
        :root {
            --primary: #1e88e5;
            --primary-dark: #1565c0;
            --secondary: #e3f2fd;
            --background: #ffffff;
            --text: #263238;
            --text-light: #546e7a;
            --accent: #43a047;
            --light-gray: #f8f9fa;
            --border-radius: 16px;
        }

        body {
            font-family: 'Roboto', sans-serif;
            color: var(--text);
            overflow-x: hidden;
        }

        h1,
        h2,
        h3,
        h4,
        h5,
        h6 {
            font-family: 'Poppins', sans-serif;
            font-weight: 700;
        }

        .section-padding {
            padding: 80px 0;
        }

        .bg-light-alt {
            background-color: var(--light-gray);
        }

        .text-primary-custom {
            color: var(--primary);
        }

        /* Common Section Header */
        .section-header {
            margin-bottom: 50px;
        }

        .section-subtitle {
            color: var(--primary);
            font-weight: 700;
            text-transform: uppercase;
            letter-spacing: 2px;
            font-size: 0.9rem;
            display: block;
            margin-bottom: 10px;
        }

        .section-title {
            font-size: 2.5rem;
            color: var(--text);
            position: relative;
            display: inline-block;
        }

        /* Hero Section */
        .page-header {
            background: linear-gradient(135deg, var(--secondary) 0%, #ffffff 100%);
            padding: 100px 0 80px;
            position: relative;
        }

        .breadcrumb-item a {
            color: var(--text-light);
            text-decoration: none;
        }

        .breadcrumb-item.active {
            color: var(--primary);
            font-weight: 600;
        }

        /* Image Styling */
        .img-frame {
            position: relative;
            border-radius: var(--border-radius);
            overflow: hidden;
            box-shadow: 0 20px 40px rgba(0, 0, 0, 0.1);
        }

        .img-frame img {
            width: 100%;
            height: 100%;
            object-fit: cover;
            transition: transform 0.5s ease;
        }

        .img-frame:hover img {
            transform: scale(1.05);
        }

        /* History Section */
        .history-year {
            font-size: 4rem;
            font-weight: 900;
            color: rgba(30, 136, 229, 0.1);
            line-height: 1;
            position: absolute;
            top: -30px;
            left: -20px;
            z-index: -1;
        }

        .history-content {
            position: relative;
            z-index: 1;
        }

        /* Vision & Mission - Redesigned */
        .vm-card {
            padding: 40px;
            border-radius: 24px;
            border: none;
            box-shadow: none;
            color: #fff;
            margin-bottom: 24px;
            transition: transform 0.3s ease;
        }

        .vm-card:last-child {
            margin-bottom: 0;
        }

        .vm-card:hover {
            transform: translateY(-5px);
        }

        .vm-card-mission {
            background-color: var(--primary);
            /* Blue */
        }

        .vm-card-vision {
            background-color: var(--accent);
            /* Green */
        }

        .vm-icon-box {
            margin-bottom: 20px;
            font-size: 2.5rem;
            color: #fff;
            opacity: 0.9;
        }

        .vm-title {
            font-size: 1.5rem;
            font-weight: 700;
            margin-bottom: 15px;
        }

        .vm-desc {
            font-size: 1rem;
            line-height: 1.6;
            opacity: 0.9;
            margin-bottom: 0;
        }

        .vm-img-frame {
            height: 100%;
            min-height: 500px;
            border-radius: 24px;
            overflow: hidden;
            box-shadow: 0 20px 40px rgba(0, 0, 0, 0.1);
        }

        .vm-img-frame img {
            width: 100%;
            height: 100%;
            object-fit: cover;
        }

        /* Chief Message */
        .chief-msg-box {
            background: #fff;
            padding: 50px;
            border-radius: var(--border-radius);
            box-shadow: 0 20px 50px rgba(0, 0, 0, 0.08);
            position: relative;
        }

        .quote-icon {
            font-size: 4rem;
            color: var(--secondary);
            position: absolute;
            top: 20px;
            left: 30px;
        }

        .chief-signature {
            font-family: 'Cursive', sans-serif;
            /* Fallback */
            font-size: 1.5rem;
            color: var(--primary);
            margin-top: 20px;
        }

        /* Why Choose Us */
        .feature-card {
            background: #fff;
            padding: 30px;
            border-radius: var(--border-radius);
            text-align: center;
            transition: all 0.3s ease;
            height: 100%;
            border: 1px solid transparent;
        }

        .feature-card:hover {
            border-color: var(--secondary);
            box-shadow: 0 15px 30px rgba(30, 136, 229, 0.1);
            transform: translateY(-10px);
        }

        .feature-icon {
            width: 70px;
            height: 70px;
            background: linear-gradient(135deg, var(--secondary) 0%, #ffffff 100%);
            color: var(--primary);
            border-radius: 20px;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 1.8rem;
            margin: 0 auto 20px;
            transition: all 0.3s ease;
        }

        .feature-card:hover .feature-icon {
            background: var(--primary);
            color: #fff;
            transform: rotate(15deg);
        }

        /* Counter Text Styling */
        .counter-number {
            font-size: 3.5rem;
            font-weight: 800;
            color: var(--primary);
            margin-bottom: 5px;
            line-height: 1;
        }

        .counter-label {
            color: var(--text-light);
            font-weight: 600;
            letter-spacing: 1px;
            text-transform: uppercase;
            font-size: 0.9rem;
        }

        /* Counter Box - Refined Clean Style */
        .counter-box {
            background: #fff;
            border: 1px solid rgba(0, 0, 0, 0.1);
            border-radius: 20px;
            padding: 40px 20px;
            text-align: center;
            transition: transform 0.4s ease, box-shadow 0.4s ease;
            height: 100%;
            position: relative;
            overflow: hidden;
            box-shadow: 0 10px 30px rgba(0, 0, 0, 0.05);
        }

        .counter-box::after {
            content: '';
            position: absolute;
            bottom: 0;
            left: 0;
            width: 100%;
            height: 4px;
            background: var(--primary);
            transform: scaleX(0);
            transform-origin: left;
            transition: transform 0.4s ease;
        }

        .counter-box:hover {
            transform: translateY(-5px);
            box-shadow: 0 20px 40px rgba(0, 0, 0, 0.12);
            border-color: var(--primary);
        }

        .counter-icon {
            width: 70px;
            height: 70px;
            background: #fff;
            color: var(--primary);
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 2rem;
            margin: 0 auto 20px;
            box-shadow: 0 10px 20px rgba(0, 0, 0, 0.1);
            transition: transform 0.3s ease;
        }

        /* Counter Icon */
        .counter-icon {
            width: 70px;
            height: 70px;
            background: #fff;
            color: var(--primary);
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 2rem;
            margin: 0 auto 20px;
            box-shadow: 0 10px 20px rgba(0, 0, 0, 0.1);
            transition: transform 0.3s ease;
        }

        .counter-box:hover .counter-icon {
            transform: scale(1.1) rotate(10deg);
            background: var(--primary);
            color: #fff;
        }



        /* Our Awards Section */
        .award-card {
            background: #fff;
            border: 1px solid rgba(0, 0, 0, 0.08);
            border-radius: 16px;
            overflow: hidden;
            transition: all 0.3s ease;
            height: 100%;
            position: relative;
        }

        .award-card:hover {
            transform: translateY(-5px);
            box-shadow: 0 15px 30px rgba(30, 136, 229, 0.15);
            border-color: var(--primary);
        }

        .award-img-box {
            height: 220px;
            position: relative;
            overflow: hidden;
            background: var(--light-gray);
        }

        .award-img-box img {
            width: 100%;
            height: 100%;
            object-fit: cover;
            transition: transform 0.5s ease;
            filter: grayscale(100%);
        }

        .award-card:hover .award-img-box img {
            transform: scale(1.05);
            filter: grayscale(0%);
        }

        .award-badge {
            position: absolute;
            top: 15px;
            right: 15px;
            width: 45px;
            height: 45px;
            background: #fff;
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 1.1rem;
            color: var(--primary);
            box-shadow: 0 5px 15px rgba(0, 0, 0, 0.1);
            z-index: 2;
        }

        .award-card:hover .award-badge {
            background: var(--accent);
            color: #fff;
        }

        .award-content {
            padding: 25px;
            text-align: center;
        }

        .award-title {
            font-size: 1.1rem;
            font-weight: 700;
            margin-bottom: 8px;
            color: var(--text);
        }

        .award-year {
            display: inline-block;
            font-size: 0.85rem;
            font-weight: 600;
            color: var(--primary);
            background: var(--secondary);
            padding: 4px 12px;
            border-radius: 20px;
        }

        /* Responsive Adjustments */
        @media (max-width: 991px) {
            .section-padding {
                padding: 50px 0;
            }

            .section-title {
                font-size: 2rem;
            }

            .chief-msg-box {
                padding: 30px;
            }

            .history-year {
                font-size: 3rem;
            }

            .vm-img-frame {
                min-height: 300px;
                margin-bottom: 24px;
            }
        }
    </style>



    <!-- Header / Hero  -->
    <header class="page-header text-center">
        <div class="container">
            <h1 class="display-4 fw-bold text-dark mb-3">About Us</h1>
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb justify-content-center">
                    <li class="breadcrumb-item"><a href="/">Home</a></li>
                    <li class="breadcrumb-item active" aria-current="page">About Us</li>
                </ol>
            </nav>
        </div>
    </header>

    <!-- 1. History + Photo Section -->
    <section class="section-padding" id="history">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-6 mb-5 mb-lg-0">
                    <div class="history-content pe-lg-4">
                        <span class="section-subtitle">Our Journey</span>
                        <h2 class="section-title mb-4">
                            <%= aboutinfo[0].about_title %>
                        </h2>
                        <div class="position-relative">
                            <p class="text-secondary justify-content-center">
                                <%- aboutinfo[0].about_desc %>
                            </p>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="img-frame">
                        <!-- Placeholder for History Photo -->
                        <img src="/uploads/<%= aboutinfo[0].about_image %>"
                            alt="NextGen IVF History" class="img-fluid">
                    </div>
                </div>
            </div>
        </div>
    </section>

